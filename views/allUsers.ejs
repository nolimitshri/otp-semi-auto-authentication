<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Users</title>
</head>
<body>
    <table>
        <th>
            <td>Email</td>
            <td>Name</td>
            <td>Age</td>
            <td>Company</td>
        </th>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.email %></td>
                <td><%= user.name %></td>
                <td><%= user.age %></td>
                <td><%= user.company %></td>
            </tr>
        <% }); %> 
    </table>
</body>
</html>